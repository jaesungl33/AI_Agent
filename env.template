# GDD RAG Backbone Environment Variables
# Copy this file to .env and fill in your API keys

# Qwen/DashScope API Configuration
QWEN_API_KEY=your_qwen_api_key_here
QWEN_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1

# Vertex AI / Gemini Configuration (optional)
VERTEX_PROJECT_ID=your_vertex_project_id_here
VERTEX_LOCATION=us-central1

# OpenAI Configuration (optional)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1

# Model defaults (optional, can override)
DEFAULT_LLM_MODEL=qwen-max
DEFAULT_EMBEDDING_MODEL=text-embedding-v2

